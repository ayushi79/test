<span style="color: #C792EA">class</span><span style="color: #FFFFFF"> </span><span style="color: #FFCB6B">AccountsConfig</span><span style="color: #89DDFF">(</span><span style="color: #C3E88D">AppConfig</span><span style="color: #89DDFF">):</span>
<span style="color: #FFFFFF">    name </span><span style="color: #C792EA">=</span><span style="color: #FFFFFF"> </span><span style="color: #89DDFF">'</span><span style="color: #C3E88D">accounts</span><span style="color: #89DDFF">'</span>

<span style="color: #FFFFFF">    </span><span style="color: #C792EA">def</span><span style="color: #FFFFFF"> </span><span style="color: #82AAFF">ready</span><span style="color: #89DDFF">(</span><span style="color: #FF5370">self</span><span style="color: #89DDFF">):</span>
