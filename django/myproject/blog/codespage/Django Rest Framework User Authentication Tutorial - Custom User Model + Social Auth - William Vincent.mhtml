.books-button { background-color: rgb(73, 168, 78); color: white; padding: 5px 15px; border-radius: 5px; }

.books-button:hover { background-color: rgb(0, 105, 217); text-decoration: none; }

a.books-button:hover { color: white; }

#mc_embed_signup { text-align: center; border: 1px solid rgb(46, 159, 255); border-radius: 5px; background: rgb(46, 159, 255); clear: left; width: 100%; margin-bottom: 30px; }

#mc_embed_signup label { display: block; margin-top: 15px; color: rgb(255, 255, 255); }

#mc_embed_signup .email { width: 80%; height: 45px; font-size: 20px; margin: 10px 0px; padding-left: 10px; border: 0px; border-radius: 5px; font-family: Concourse_T3; }

#mc-embedded-subscribe { padding: 5px 15px; background-color: rgb(12, 196, 113); color: white; border-radius: 5px; font-size: 20px; margin-bottom: 20px; margin-top: 15px; border: 1px solid rgb(12, 196, 113); font-family: Concourse_T3; }

#carbonads { display: flex; float: right; margin: 0px 0px 20px 20px; max-width: 130px; border-radius: 4px; background-color: rgb(250, 250, 250); box-shadow: rgba(0, 0, 0, 0.15) 0px 0px 1px; font-size: 11px; font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen-Sans, Ubuntu, Cantarell, "Helvetica Neue", Helvetica, Arial, sans-serif; }

#carbonads a { color: rgb(17, 17, 17); text-decoration: none; }

#carbonads a:hover { color: rgb(17, 17, 17); }

.carbon-img { display: block; margin-bottom: 8px; max-width: 130px; line-height: 1; }

.carbon-img img { display: block; margin: 0px auto; max-width: 130px; width: 130px; height: auto; }

.carbon-text { display: block; padding: 0px 10px 8px; text-align: left; line-height: 1.35; }

.carbon-poweredby { display: block; padding: 10px; background: repeating-linear-gradient(-45deg, transparent, transparent 5px, rgba(0, 0, 0, 0.024) 5px, rgba(0, 0, 0, 0.024) 10px) rgba(241, 243, 244, 0.4); text-transform: uppercase; letter-spacing: 0.5px; font-weight: 600; font-size: 8px; line-height: 0; }

@media only screen and (max-width: 759px) and (min-width: 320px) {
  #carbonads { position: relative; float: none; margin: 20px 0px; max-width: 330px; }
  .carbon-wrap { display: flex; flex-direction: row; }
  .carbon-img { margin: 0px; }
  .carbon-text { padding: 10px 10px 0px; font-size: 12px; }
  .carbon-poweredby { position: absolute; right: 0px; bottom: 0px; border-radius: 3px 0px 0px; text-align: center; }
}

