.page-purchase-bundle .purchase-details { padding-bottom: 0px; }

.purchase-details { padding: 60px 0px 40px; background: rgb(255, 255, 255); }

.purchase-back-link { position: relative; display: inline-block; margin-bottom: 30px; font-weight: 700; color: var(--brand1); }

.purchase-back-link:hover { color: var(--brand2); }

.cart-tally { display: flex; flex-flow: row wrap; justify-content: space-between; align-items: center; padding: 10px var(--spacer); border: 8px solid var(--light-gray-alt); }

.cart-price { display: block; font-size: 3rem; font-weight: 600; }

.qty-field { display: flex; flex-flow: row wrap; align-items: center; }

.cart-tally label { display: inline-block; margin: 0px 0.35rem 0px 0px; font-size: 0.8rem; font-weight: 600; text-transform: uppercase; color: var(--gray); }

.cart-tally label abbr { border-bottom: none !important; cursor: inherit !important; text-decoration: none !important; }

.cart-tally select { width: 7ch; }

.cart-tally .field-tooltip { display: inline-block; padding: 0.35em; }

.page-purchase .course-card-details .bundle-buy { display: none; }

.page-purchase .bundle-buy.wide { display: block; }

.payment-block { padding: 60px 0px; background: var(--light-gray); }

.payment-form { margin: 0px auto 80px; max-width: 540px; }

.payment-form .form-actions { text-align: right; }

.payment-form .btn { min-width: 204px; }

.payment-form .loader { display: inline-block; margin: 0px 0.5em 0px 0px; }

.payment-form-heading { margin-top: 0px; font-size: 1.6rem; }

.purchase-btn { height: 46px; }

.payment-errors { margin-top: 8px; padding: 0.5rem 1rem; font-size: 0.9rem; }

.payment-errors:empty { display: none; }

.payment-form .lead { margin-bottom: 0px; }

.StripeElement { padding: 10px 15px; width: 100%; min-height: 43px; }

.purchase-faqs { margin-top: calc(var(--spacer) * 3); padding-top: calc(var(--spacer) * 3); border-top: 1px solid rgba(0, 0, 0, 0.1); }

.bundle-card-text, .bundle-course-list { font-size: 0.8rem; }

.page-purchase .bundle-card-cover .bundle-card-img { top: 30px; right: 30px; left: auto; z-index: 1; width: 60%; transform: rotate(0deg); }

.page-purchase .bundle-card-cover .bundle-card-img:first-child { top: 60px; right: 60px; left: auto; z-index: 2; }

.page-purchase .bundle-card-cover .bundle-card-img:last-child { top: 0px; right: 10px; z-index: 0; margin-left: 0px; }

.confirmation-header { margin-bottom: calc(var(--spacer) * 3); padding-top: 25px; padding-bottom: 25px; border: 1px solid var(--light-gray-alt); }

.cart-seats { margin-bottom: 0px; color: var(--gray); }

.cart-seats strong { display: block; }

.seats-cta, .seats-discount-info { font-size: 0.8rem; }

.seats-discount-info { width: 100%; line-height: 1.8; }

.page-confirmation .cart-price { margin-right: var(--spacer); }

@media (min-width: 768px) {
  .page-purchase .course-card-heading { font-size: 1.5rem; }
  .page-purchase .course-card-attrs { margin-bottom: var(--spacer); }
  .cart-tally { display: block; padding: var(--spacer) 6px 20px var(--spacer); }
  .cart-price { font-size: 2rem; }
  .cart-seats { margin-bottom: 6px; }
  .page-purchase .course-card-details .bundle-buy { display: inline-block; }
  .page-purchase .bundle-buy.wide { display: none; }
  .page-confirmation .cart-price { margin-right: 0px; }
}

@media (min-width: 992px) {
  .cart-price { font-size: 3rem; }
  .bundle-buy { font-size: 0.9rem; }
  .page-purchase .course-card-attrs { display: flex; }
  .page-purchase .course-card-attrs .course-card-author { margin-right: 30px; margin-bottom: 0px; }
}
