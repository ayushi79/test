&nbsp; &nbsp; &nbsp; &nbsp; cur.<span class="me1">execute</span><span class="br0">(</span><span class="st0">"Truncate {} Cascade;"</span>.<span class="me1">format</span><span class="br0">(</span>table_name<span class="br0">)</span><span class="br0">)</span><br>
&nbsp; &nbsp; &nbsp; &nbsp; <span class="kw1">print</span><span class="br0">(</span><span class="st0">"Truncated {}"</span>.<span class="me1">format</span><span class="br0">(</span>table_name<span class="br0">)</span><span class="br0">)</span><br>
