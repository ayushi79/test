&nbsp; &nbsp; create_table<span class="br0">(</span>rest_records<span class="sy0">,</span> rest_headers<span class="sy0">,</span> <span class="st0">'./restaurants.csv'</span><span class="br0">)</span><br>
&nbsp; &nbsp; <span class="co1"># (5) Retrive all data again for grades</span><br>
&nbsp; &nbsp; results <span class="sy0">=</span> restaurants.<span class="me1">find</span><span class="br0">(</span><span class="br0">)</span><br>
&nbsp; &nbsp; <span class="co1"># (5-1) Transform data for grades</span><br>
&nbsp; &nbsp; grades_tup <span class="sy0">=</span> create_grades<span class="br0">(</span>results<span class="br0">)</span><br>
