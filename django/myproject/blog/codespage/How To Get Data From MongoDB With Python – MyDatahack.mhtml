.news-posts { font-size: 16px; line-height: 1.8; color: rgba(0, 0, 0, 0.92); margin-right: 10px; }

.news a { color: rgb(51, 122, 183) !important; }

.navbar-brand { font-family: "Fredoka One", cursive; letter-spacing: 2px; color: rgb(221, 51, 51) !important; }

.hr-custom { border: none; height: 1px; background-image: linear-gradient(to right, rgb(240, 240, 240), rgb(143, 143, 143), rgb(240, 240, 240)); margin-right: 20px; margin-top: 10px; }

.hr-custom::after { content: "§"; display: inline-block; position: absolute; left: 50%; transform: translate(-50%, -50%) rotate(60deg); transform-origin: 50% 50%; padding: 1rem; background-color: rgb(255, 255, 255); }

.gist .blob-code-inner { font-size: 14px !important; }

pre { font-size: 14px; }

.codecolorer { margin: 10px 5px; font-size: 15px !important; line-height: 1.6px !important; }

.codecolorer { font-size: 15px !important; }

.codecolorer span { font-size: 15px !important; }

.line-numbers { display: none; }

@keyframes bounce { 
  20%, 53%, 80%, 0%, 100% { animation-timing-function: cubic-bezier(0.215, 0.61, 0.355, 1); transform: translate3d(0px, 0px, 0px); }
  40%, 43% { animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06); transform: translate3d(0px, -15px, 0px); }
  70% { animation-timing-function: cubic-bezier(0.755, 0.05, 0.855, 0.06); transform: translate3d(0px, -8px, 0px); }
  90% { transform: translate3d(0px, -4px, 0px); }
}

.bounce { animation-name: bounce; transform-origin: center bottom; }

.animated { animation-duration: 0.7s; animation-fill-mode: both; }

.animated-brand-ttl { display: flex; flex-direction: row; }

.gist-meta { display: none; }

@font-face { font-family: FontAwesome; src: url("../fonts/fontawesome-webfont.eot?#iefix&v=4.7.0") format("embedded-opentype"), url("../fonts/fontawesome-webfont.woff2?v=4.7.0") format("woff2"), url("../fonts/fontawesome-webfont.woff?v=4.7.0") format("woff"), url("../fonts/fontawesome-webfont.ttf?v=4.7.0") format("truetype"), url("../fonts/fontawesome-webfont.svg?v=4.7.0#fontawesomeregular") format("svg"); font-weight: 400; font-style: normal; }

.fa { display: inline-block; font-style: normal; font-variant: normal; font-weight: normal; font-stretch: normal; line-height: 1; font-family: FontAwesome; font-size: inherit; text-rendering: auto; -webkit-font-smoothing: antialiased; }

.fa-search::before { content: ""; }

.nav-burger-container { margin-top: 6px; }

.nav-burger { border: 1px solid rgb(221, 51, 51); width: 22px; position: relative; border-radius: 1px; }

.nav-burger::before { content: ""; position: absolute; border: 1px solid rgb(221, 51, 51); width: 22px; top: -7px; left: -1px; }

.nav-burger::after { content: ""; position: absolute; border: 1px solid rgb(221, 51, 51); width: 22px; top: 5px; left: -1px; }

@keyframes moveDown { 
  0% { transform: translateY(0px); opacity: 1; }
  30% { transform: translateY(7px); opacity: 1; }
  100% { transform: translateY(7px) rotate(-90deg); transform-origin: center center; opacity: 1; }
}

@keyframes moveDown-reverse { 
  0% { transform: translateY(7px) rotate(-90deg); transform-origin: center center; opacity: 1; }
  30% { transform: translateY(7px); opacity: 1; }
  100% { transform: translateY(0px); opacity: 1; }
}

@keyframes moveUp { 
  0% { transform: translateY(0px); opacity: 1; }
  30% { opacity: 0; }
  100% { transform: translateY(-5px); opacity: 0; }
}

@keyframes moveUp-reverse { 
  0% { transform: translateY(-5px); opacity: 0; }
  30% { transform: translateY(-5px); opacity: 0; }
  100% { transform: translateY(0px); opacity: 1; }
}

@keyframes rotate { 
  0% { transform: translateY(0px); }
  30% { transform: translateY(0px); }
  100% { transform: rotate(45deg); }
}

@keyframes rotate-reverse { 
  0% { transform: rotate(45deg); }
  30% { transform: translateY(0px); }
  100% { transform: translateY(0px); }
}

.nav-burger.animate::before { animation: 0.4s cubic-bezier(0.52, 0.14, 0.42, 0.89) 0s 1 normal both running moveDown; }

.nav-burger.animate::after { animation: 0.4s cubic-bezier(0.52, 0.14, 0.42, 0.89) 0s 1 normal both running moveUp; }

.nav-burger.rotate { animation: 0.4s cubic-bezier(0.52, 0.14, 0.42, 0.89) 0s 1 normal both running rotate; }

.nav-burger.animate-reverse::before { animation: 0.4s cubic-bezier(0.52, 0.14, 0.42, 0.89) 0s 1 normal both running moveDown-reverse; }

.nav-burger.animate-reverse::after { animation: 0.4s cubic-bezier(0.52, 0.14, 0.42, 0.89) 0s 1 normal both running moveUp-reverse; }

.nav-burger.rotate-reverse { animation: 0.4s cubic-bezier(0.52, 0.14, 0.42, 0.89) 0s 1 normal both running rotate-reverse; }

.mdh-custom-archive { height: 210px; overflow: hidden; position: relative; }

.mdh-custom-archive.active { height: 100%; }

.archive-show-more { color: rgb(221, 51, 51); font-size: 15px; position: relative; top: -39px; padding-top: 40px; z-index: 10; padding-left: 40px; background-image: linear-gradient(rgba(255, 255, 255, 0), rgb(255, 255, 255)); }

.archive-show-more:hover { font-size: 16px; transition: all 0.1s ease-in-out 0s; cursor: pointer; }

.archive-show-more.active { background-image: linear-gradient(rgba(255, 255, 255, 0), rgba(255, 255, 255, 0)); }
