<span class="na">Host</span><span class="o">:</span> <span class="l">connect.example.com</span>
<span class="na">Content-Type</span><span class="o">:</span> <span class="l">application/json</span>
<span class="na">Accept</span><span class="o">:</span> <span class="l">application/json</span>

<span class="p">{</span>
    <span class="nt">"name"</span><span class="p">:</span> <span class="s2">"hdfs-sink-connector"</span><span class="p">,</span>
    <span class="nt">"config"</span><span class="p">:</span> <span class="p">{</span>
        <span class="nt">"connector.class"</span><span class="p">:</span> <span class="s2">"io.confluent.connect.hdfs.HdfsSinkConnector"</span><span class="p">,</span>
        <span class="nt">"tasks.max"</span><span class="p">:</span> <span class="s2">"10"</span><span class="p">,</span>
        <span class="nt">"topics"</span><span class="p">:</span> <span class="s2">"test-topic"</span><span class="p">,</span>
        <span class="nt">"hdfs.url"</span><span class="p">:</span> <span class="s2">"hdfs://fakehost:9000"</span><span class="p">,</span>
        <span class="nt">"hadoop.conf.dir"</span><span class="p">:</span> <span class="s2">"/opt/hadoop/conf"</span><span class="p">,</span>
        <span class="nt">"hadoop.home"</span><span class="p">:</span> <span class="s2">"/opt/hadoop"</span><span class="p">,</span>
        <span class="nt">"flush.size"</span><span class="p">:</span> <span class="s2">"100"</span><span class="p">,</span>
        <span class="nt">"rotate.interval.ms"</span><span class="p">:</span> <span class="s2">"1000"</span>
    <span class="p">}</span>
<span class="p">}</span>
</pre><div class="copy-code-button"><svg class="icon"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#clipboard"><svg id="clipboard" viewBox="0 0 32 32" width="100%" height="100%"><g><path d="M10 12h12v4H10zM10 20h12v4H10zM18 2V0h-4v2H2v30h28V2H18zm8 26H6V6h4v2h12V6h4v22z"></path></g></svg><template shadowmode="closed"><svg id="clipboard" viewBox="0 0 32 32" width="100%" height="100%"><g><path d="M10 12h12v4H10zM10 20h12v4H10zM18 2V0h-4v2H2v30h28V2H18zm8 26H6V6h4v2h12V6h4v22z"></path></g></svg></template></use></svg><span>Copy</span></div></div>
